<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.cori.typemaster.GameController" stylesheets="@style.css">

  <Label fx:id="StartButton"/>
  <Button text="Start Game" onAction="#onStartButtonClick"/>

  <Label fx:id="ExitButton"/>
  <Button text="Quit Game" onAction="#setExitButtonClick"/>
</VBox>
